{
  "name": "mooza-social-network",
  "version": "1.0.0",
  "description": "Mooza Music Social Network - A platform for musicians to connect and collaborate",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=backend\" \"npm start --workspace=frontend\"",
    "build": "npm run build --workspace=frontend",
    "test": "npm test --workspaces",
    "deploy": "npm run deploy --workspace=frontend",
    "deploy:gh": "npm run deploy:gh --workspace=frontend",
    "deploy:vps": "node deployment/deploy-to-vps.js",
    "lint": "npm run lint --workspaces",
    "clean": "rimraf node_modules && npm run clean --workspaces"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptoChaosDev/mooza_dev.git"
  },
  "keywords": [
    "music",
    "social-network",
    "react",
    "nodejs",
    "express",
    "prisma",
    "typescript"
  ],
  "author": "Crypto Chaos Dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cryptoChaosDev/mooza_dev/issues"
  },
  "homepage": "https://github.com/cryptoChaosDev/mooza_dev#readme"
}